#include <stdio.h>
#pragma warning(disable:4996)
#include <string.h>
#include <stdlib.h>

typedef struct TW {
    int height;
    int num;
}TW;

typedef struct Stack {
    TW *arr;
    int top;
    int capacity;
}Stk;

int init(Stk* s, int capacity) {
    s->top = -1;
    s->capacity = capacity;
    s->arr = (TW*)malloc(capacity * sizeof(TW));
    if (s->arr == NULL)
        return 0;
    return 1;
}

int is_full(Stk* s) {
    if (s->top == s->capacity - 1)
        return 1;
    return 0;
}

int is_empty(Stk* s) {
    if (s->top == -1)
        return 1;
    return 0;
}

int push(Stk* s, TW N) {
    if (is_full(s)) {
        printf("Stack is FULL");
        return 0;
    }
    s->arr[++(s->top)] = N;
    return 1;
    
}

int pop(Stk* s, TW *out) {
    if (is_empty(s)) {
        printf("Stack is empty");
        return 0;
    }
    *out = s->arr[(s->top)--];
    return 1;
}

int peek(Stk* s, TW *out) {
    if (is_empty(s)) {
        printf("Stack is empty");
        return 0;
    }
    *out = s->arr[(s->top)];
    return 1;
}

void destroy(Stk* s) {
    free(s->arr);
    s->arr = NULL; //버그차단
}

void tower(Stk* s) {
    TW a, tmp;
    for (int i = 0; i < s->capacity; i++) {
        scanf("%d", &a.height);
        a.num = i+1;
        if (i == 0) {
            push(s, a);
            printf("0 ");
        }
        else {
            while (s->arr[s->top].height < a.height && s->top > -1) {
                pop(s, &tmp);
            }
            if (s->top == -1)
                printf("0 ");
            else
                printf("%d ", s->arr[s->top].num);
            push(s, a);
        }
    }
    
    destroy(s);
}

int main() {
    Stk s;
    int n;
    scanf("%d", &n);
    init(&s, n);
    tower(&s);

    return 0;
}
