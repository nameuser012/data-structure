#include <stdio.h>
#pragma warning(disable:4996)
#include <string.h>
#include <stdlib.h>
#define FULL 100

typedef struct Stack {
    double arr[100];
    int top;
}Stk;

void init(Stk* s) {
    s->top = -1;
}

int is_full(Stk* s) {
    if (s->top == FULL - 1)
        return 1;
    return 0;
}

int is_empty(Stk* s) {
    if (s->top == -1)
        return 1;
    return 0;
}

void push(Stk* s, double N) {
    if (is_full(s)) {
        printf("Stack is FULL");
        exit(1);
    }
    s->arr[++(s->top)] = N;
}

double pop(Stk* s) {
    if (is_empty(s)) {
        printf("Stack is empty");
        exit(1);
    }
    return s->arr[s->top--];
}

double peek(Stk* s) {
    if (is_empty(s)) {
        printf("Stack is empty");
        exit(1);
    }
    return s->arr[s->top];
}

void postfix_math(Stk* s) {
    int N,num[26], i=0;
    scanf("%d", &N);
    getchar();
    char  ch[100];
    scanf("%c", &ch[i++]);

    while (ch[i-1] != '\n') {
        scanf("%c", &ch[i++]);
    }

    for (int j = 0; j < N; j++)
        scanf("%d", &num[j]);

    double pop1, pop2;
    int l;
    for (int j = 0; j < i; j++) {
        if (ch[j] >= 'A' && ch[j] <= 'Z') {
            l = (int)(ch[j] - 'A');
            push(s, num[l]);
        }
        else if (ch[j] == '+') {
            pop2 = pop(s);
            pop1 = pop(s);
            push(s, pop1 + pop2);
        }
        else if (ch[j] == '-') {
            pop2 = pop(s);
            pop1 = pop(s);
            push(s, pop1 - pop2);
        }
        else if (ch[j] == '*') {
            pop2 = pop(s);
            pop1 = pop(s);
            push(s, pop1 * pop2);
        }
        else if (ch[j] == '/') {
            pop2 = pop(s);
            pop1 = pop(s);
            push(s, pop1 / pop2);
        }
    }
    printf("%.2f", pop(s));
}

int main() {
    Stk s;
    init(&s);
    postfix_math(&s);
    return 0;
}
